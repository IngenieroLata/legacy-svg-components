{
  "name": "legacy-svg-components",
  "version": "0.0.1",
  "description": "craco-plugin to load svg on react 15",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix lib",
    "format": "prettier --write **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IngenieroLata/legacy-svg-components.git"
  },
  "keywords": [
    "craco",
    "react15",
    "plugin.",
    "svg",
    "legacy"
  ],
  "author": "IngenieroLata",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IngenieroLata/legacy-svg-components/issues"
  },
  "homepage": "https://github.com/IngenieroLata/legacy-svg-components#readme",
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "eslint": "6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.0",
    "prettier": "^1.15.3",
    "react-scripts": "^3.3.1",
    "webpack": "4.41.5"
  },
  "dependencies": {
    "svg-react-loader": "^0.4.6"
  },
  "peerDependencies": {
    "@craco/craco": "^5.5.0",
    "react-scripts": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
